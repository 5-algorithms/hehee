    "def solution(n):\n",
    "    answer = 0\n",
    "    x = n**(1/2)\n",
    "    if x % 1 ==0:\n",
    "        answer = (x+1)**2\n",
    "    else:\n",
    "        answer = -1\n",
    "    return answer"
