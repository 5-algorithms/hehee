    "def solution(citations):\n",
    "    citations.sort()\n",
    "    article_count = len(citations)\n",
    "\n",
    "    for i in range(article_count):\n",
    "        if citations[i] >= article_count-i:\n",
    "            return article_count-i\n",
    "    return 0"
   ]
