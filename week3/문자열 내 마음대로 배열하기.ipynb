    "def solution(strings, n):\n",
    "    str = []\n",
    "    for i in strings:\n",
    "        str.append(i[n]+i)\n",
    "    str.sort()\n",
    "    \n",
    "    answer = []\n",
    "    for i in str:\n",
    "        answer.append(i[1:])\n",
    "    \n",
    "    return answer"
   ]
