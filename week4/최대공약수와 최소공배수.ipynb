    "def solution(n, m):\n",
    "    answer=[]\n",
    "    for i in range(min(n, m), 0, -1):\n",
    "        if n%i==0 and m%i==0:\n",
    "            answer.append(i)\n",
    "            break\n",
    "    for i in range(max(n, m), (n*m)+1):\n",
    "        if i%n==0 and i%m==0:\n",
    "            answer.append(i)\n",
    "            break\n",
    "    return answer"
   ]
  }
