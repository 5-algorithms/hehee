    "def solution(a, b):\n",
    "    answer=0\n",
    "    if a==b:\n",
    "        answer=a or b\n",
    "    else:\n",
    "        for i in range(min(a,b), max(a,b)+1):\n",
    "            answer+=i\n",
    "    return answer"
