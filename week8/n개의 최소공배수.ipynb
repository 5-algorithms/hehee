   "source": [
    "def solution(arr):\n",
    "    from math import gcd\n",
    "    answer = arr[0]\n",
    "\n",
    "    for num in arr:\n",
    "        answer = answer*num // gcd(answer, num)     \n",
    "\n",
    "    return answer"
   ]
  }
